<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.japhethnyaranga.my_portfolioapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyPortfolioApp">
        <activity android:name="com.japhethnyaranga.my_portfolioapp.SkillsActivity"></activity>
        <activity
            android:name="com.japhethnyaranga.my_portfolioapp.ContactActivity"
            android:exported="true"></activity>
        <activity android:name="com.japhethnyaranga.my_portfolioapp.PortfolioActivity" />
        <activity android:name="com.japhethnyaranga.my_portfolioapp.ServicesActivity" />
        <activity
            android:name="com.japhethnyaranga.my_portfolioapp.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>